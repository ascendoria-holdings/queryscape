{
  "name": "@queryscape/cli",
  "version": "0.1.0",
  "description": "CLI tool for QueryScape",
  "keywords": ["graph", "cli", "visualization"],
  "license": "MIT",
  "author": "QueryScape Contributors",
  "type": "module",
  "bin": {
    "queryscape": "./dist/index.js"
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "clean": "rm -rf dist *.tsbuildinfo",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@queryscape/connectors": "workspace:*",
    "@queryscape/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3",
    "vitest": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
